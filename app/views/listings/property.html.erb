<div id="page-content"  style="margin-top: 150px">
        <!-- Breadcrumb -->
        <div class="container">
            <ol class="breadcrumb">
                <li><a href="#">Home</a></li>
                <li><a href="#">Account</a></li>
                <li class="active">My Properties</li>
            </ol>
        </div>
        <!-- end Breadcrumb -->

        <div class="container">
            <div class="row">
            <!-- sidebar -->
            <div class="col-md-3 col-sm-2">
                <section id="sidebar">
                    <header><h3>Account</h3></header>
                    <aside>
                        <ul class="sidebar-navigation">
                            <li ><a href="/edit"><i class="fa fa-user"></i><span>Profile</span></a></li>
                            <li class="active"><a href="/property" ><i class="fa fa-home"></i><span>My Properties</span></a></li>
                            <li ><a href="/listings"><i class="fa fa-upload"></i><span>Upload Property</span></a></li>
                            <li> <a href="<%= destroy_user_session_path %>" data-method="delete" ><i class="fa fa-key"></i><span>Logout</span></a> </li>
                        </ul>
                    </aside>
                </section><!-- /#sidebar -->
            </div><!-- /.col-md-3 -->
            <!-- end Sidebar -->
                <!-- My Properties -->
                <div class="col-md-9 col-sm-10">
                    <section id="my-properties">
                        <header><h1>My Properties</h1></header>
                        <div class="my-properties">
                            <div class="table-responsive">
                                <table class="table">
                                    <thead>
                                    <tr>
                                        <th>Property</th>
                                        <th></th>
                                        <th>Date Added</th>
                                        <th>Views</th>
                                        <th>Actions</th>
                                    </tr>
                                    </thead>
                                    <tbody>
                                         <% @records.each do |buy| %>
                                  <% image = buy.listing_images.first %>
                                    
                                        <tr>
                                        <td class="image">
                                           <a href="/listings/<%= buy.id %>">
                                       <%= image_tag image.img.url, :class => "style_image"%>
                                    </a>
                                        </td>
                                       <td><div class="inner">
                                            <a href="#"><h2><%= buy.title %></h2></a>
                                               <div class="tag price">â‚¦<%= buy.price %></div>
                                        </div>
                                        </td>
                                        <td><%= buy.created_at %> </td>
                                        <!--<td>236</td> -->
                                        <td class="actions">
                                            <a href="#" class="edit"><i class="fa fa-pencil"></i>Edit</a>
                                            <a href="#"><i class="delete fa fa-trash-o"></i></a>
                                        </td>  
                                    </tr>  
                                       <% end %>

                                                                        
                                    </tbody>
                                </table>
                            </div><!-- /.table-responsive -->
                            <!-- Pagination -->
                            <div class="center">
                                <ul class="pagination">
                                    <li class="active"><a href="#">1</a></li>
                                    <li><a href="#">2</a></li>
                                    <li><a href="#">3</a></li>
                                    <li><a href="#">4</a></li>
                                    <li><a href="#">5</a></li>
                                </ul><!-- /.pagination-->
                            </div><!-- /.center-->
                        </div><!-- /.my-properties -->
                    </section><!-- /#my-properties -->
                </div><!-- /.col-md-9 -->
                <!-- end My Properties -->
            </div><!-- /.row -->
        </div><!-- /.container -->
    </div>