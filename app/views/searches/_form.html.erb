  <%= form_with model: search do |form| %>
  
    <div class="form-group">
    <%= form.label :status %>
  
   <%= form.select(:status, options_for_select([['Sale','Sale'],['Rent','Rent']])) %>
                                
                                   
  </div>


  

  <div class="form-group">
    <%= form.label :area %>
     <%= form.select(:area, options_for_select([['Mainland-Lagos','Mainland-Lagos'],['Island-Lagos','Island-Lagos']])) %>
  </div>

  
   <div class="form-group">
                                    <div class="price-range">
                                        <input id="price-input" type="text" name="price" value="100000;500000000">
                                    </div>
                                </div>

 

  <div class="form-group">
    <%= form.submit  %>
  </div>
<% end %>
